{
  "name": "SNPI Macro",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Make notion.so great again",
  "author": "",
  "homepage_url": "https://github.com/kissge/snpi-macro",
  "icons": {
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
